{
	"name": "wc-ui",
	"version": "0.0.1",
	"description": "Stencil Component Starter",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/wc-ui/wc-ui.js",
	"files": [
		"dist/",
		".temp/loader/"
	],
	"scripts": {
		"start": "npm-run-all --parallel stencil storybook",
		"prestart": "npm run stencil:build",
		"build": "npm-run-all clean stencil:build storybook:build post:build",
		"post:build": "node ./stencil-utilities/post-build.js",
		"test": "stencil test --spec --e2e",
		"test.watch": "stencil test --spec --e2e --watchAll",
		"stencil": "stencil build --dev --watch --serve",
		"stencil:build": "stencil build --docs",
		"storybook": "start-storybook -p 4300",
		"storybook:build": "build-storybook",
		"clean": "del-cli dist storybook-static",
		"generate": "plop"
	},
	"devDependencies": {
		"@babel/core": "^7.8.3",
		"@stencil/core": "^1.8.4",
		"@stencil/sass": "^1.1.1",
		"@storybook/addon-docs": "^5.3.6",
		"@storybook/html": "^5.3.6",
		"@types/jest": "^24.9.0",
		"@types/node": "^13.1.8",
		"babel-loader": "^8.0.6",
		"chalk": "^2.4.2",
		"cross-env": "^5.2.0",
		"del-cli": "^1.1.0",
		"fs-extra": "^7.0.1",
		"npm-run-all": "^4.1.5",
		"plop": "^2.2.0"
	},
	"license": "MIT",
	"dependencies": {
		"lodash": "^4.17.15"
	}
}
